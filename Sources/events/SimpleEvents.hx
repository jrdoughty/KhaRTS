package events;
/**
*Events that can get by with simple Event Objects passed into them go here
*/
class SimpleEvents
{
	public static inline var GO_TO_RESOURCE:String = "GO_TO_RESOURCE";
	public static inline var BUILD:String = "BUILD";
	public static inline var CLEAR = "CLEAR";
	public static inline var HIDE:String = "HIDE";
	public static inline var RETURN:String = "RETURN";
	public static inline var REVEAL:String = "REVEAL";
	public static inline var STOP:String = "STOP";
}